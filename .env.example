# Supabase Configuration
VITE_SUPABASE_PROJECT_ID=your_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key
VITE_SUPABASE_URL=your_supabase_url

# PostHog Analytics
VITE_POSTHOG_KEY=phc_YOUR_KEY_HERE
VITE_POSTHOG_HOST=https://app.posthog.com

# Sentry Error Tracking & Monitoring
# The DSN is public and safe to commit (it's used client-side)
# Already configured in the code, no need to set here unless you want to override
# VITE_SENTRY_DSN=https://xxxxx@o172702.ingest.us.sentry.io/xxxxx

# Sentry Build Configuration (for CI/CD and production builds)
# This token is SECRET and should NEVER be committed to git
# Set it in your CI/CD environment variables or local .env file
# Get it from: https://sentry.io/settings/account/api/auth-tokens/
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# Application Version (for release tracking in Sentry)
# This should be automatically set by your CI/CD pipeline
# Example: VITE_APP_VERSION=1.0.0 or git commit hash
VITE_APP_VERSION=development

# Google Maps API (for Supabase Edge Functions)
# Note: This key should be set as a Supabase secret, not in .env
# Run: supabase secrets set GOOGLE_MAPS_API_KEY=your_key_here
# GOOGLE_MAPS_API_KEY=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Chatwoot Live Chat Widget (Optional)
# Customer support chat integration
# Get these values from your Chatwoot instance
# VITE_CHATWOOT_WEBSITE_TOKEN=your_website_token_here
# VITE_CHATWOOT_BASE_URL=https://your-chatwoot-instance.com

# Shlink URL Shortener (Optional)
# URL shortening service for payment proof links in WhatsApp messages
# Get your API key from your Shlink instance
VITE_SHLINK_API_URL=https://x.pideai.com/rest/v3
